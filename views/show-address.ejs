<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Show Address</title>
  </head>
  <body>
    <h1>
      <table>
        <tr>
          <th>id</th>
          <th>Street</th>
          <th>City</th>
          <th>ZipCode</th>
        </tr>
        <%showAddress.forEach(addresses =>{%>
          <tr>
            <td>
              <%=addresses.id %>
            </td>
            <td>
              <%=addresses.street%>
            </td>
            <td>
              <%=addresses.city%>
            </td>
            <td>
              <%=addresses.zipCode%>
            </td>
          </tr>
        <%})%>
      </table>
    </h1>
  </body>
</html>
