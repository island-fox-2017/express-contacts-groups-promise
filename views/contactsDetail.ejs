<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Contact Details</title>
  </head>
  <body>
    <h1><%=dataGroups%></h1>
    <table border="2">
      <tr>
        <th>id</th>
        <th>Name</th>
        <th>Company</th>
        <th>Phone Number</th>
        <th>email</th>
        <th>Groups</th>
      </tr>
        <% dataGroups.forEach(contact => {%>
          <tr>
            <td>
              <%= contact.id %>
            </td>
            <td>
              <%=contact.name%>
            </td>
            <td>
              <%=contact.company%>
            </td>
            <td>
              <%=contact.telp_number%>
            </td>
            <td>
              <%=contact.email%>
            </td>
            <td><%=contact.name_of_group%></td>
            <td>
              <a href="/contacts/edit/<%=contact.id %>">Edit</a>
              <a href="/contacts/delete/<%=contact.id %>">Delete</a>
              <a href="/contacts/show-address/<%=contact.id %>">Show Address</a>
            </td>
          </tr>
        <% }) %>
    </table>
  </body>
</html>
