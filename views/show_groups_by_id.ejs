<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>show groups by id</title>
  </head>
  <body>
    <%= header %>
    <table border="1">
      <tr>
      <th>Id</th>
      <th>Group Name</th>
      </tr>
    <% data_groups.forEach( gr => { %>
      <tr>
      <td><%= gr.id %></td>
      <td><%= gr.name_of_group %></td>
      </tr>
    <% }) %>
  </table>
    <br>
    <a href="/contacts">Contacts</a>
  </body>
</html>
