<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Edit Profile Form</h1>
    <form action="/profile/edit/:id" method="post">
      <input type="text" name="id" value="<%=editProfile[0].id %>">
      <input type="text" name="username" value="<%=editProfile[0].username%>">
      <input type="text" name="password" value="<%=editProfile[0].password%>">
      <input type="text" name="contact_id" value="<%=editProfile[0].contact_id%>">
      <select name="editProfile">
        <%contactName.forEach(kontak=>{%>
          <option value="<%=kontak.id%>"><%=kontak.name%></option>
        <%})%>
      </select>
      <button type="submit" name="button">Submit</button>
    </form>
  </body>
</html>
