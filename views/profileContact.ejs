<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <style>
    table {
      font-family:Comic Extrabold, sans-serif;
      position: center;
    }

    td, th {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 1px;
    }

    tr:nth-child(even) {
      background-color: #dddddd;
    }
  </style>
  <title>Profile's contact</title>
  </head>

  <body>
  <center><h3>Profile's contact</h3><center><br>
  <center>
  <table>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Company</th>
      <th>Telp</th>
      <th>Email</th>
      <th colspan='2'></th>
    </tr>
    <% dataProfCont.forEach( ProfCont =>{%>
    <tr>
      <td><%= ProfCont.id %></td>
      <td><%= ProfCont.Name %></td>
      <td><%= ProfCont.Company %></td>
      <td><%= ProfCont.Telp %></td>
      <td><%= ProfCont.Email %></td>
      </tr>
      <%})%>
    </table>
  </center>
    </form>
  </body>
</html>
