<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Detail contacts Page</title>
    <link href="/style.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/font-awesome/css/font-awesome.min.css">
  </head>
  <body>
    <h1><i class="fa fa-address-book"> Detail Contacts </i></h1>
    <button id="btn-start" onclick="window.location='/contacts'"><i class="fa fa-arrow-left"> Back</i></button>

    <div>
      <table border="1">
        <tr>
          <th>NAME</th>
          <th>COMPANY</th>
          <th>No. PHONE</th>
          <th>EMAIL</th>
          <th>USERNAME</th>
          <th>PASSWORD</th>
          <th>ID CONTACT</th>
        </tr>
        <% data.forEach(d => {%>
          <tr>
            <td><%= d.first_name %> <%= d.last_name %></td>
            <td><%= d.company %></td>
            <td><%= d.telp_number %></td>
            <td><%= d.email %></td>
            <td><%= d.username%></td>
            <td><%= d.pwd%></td>
            <td><%= d.contact_id%></td>
          </tr>
        <% }) %>
      </table>
    </div>
    <script src="/script.js"></script>
  </body>
</html>
