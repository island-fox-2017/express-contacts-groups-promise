<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>add profile</title>
  </head>
  <body>
    <form class="" action="/profiles/add" method="post">
      <fieldset>
        <legend>Input Data Profile</legend>
        <b>Username : </b><br>
        <input type="text" name="username" value=""> <br><br>
        <b>Facebook Username : </b><br>
        <input type="text" name="facebook" value=""> <br><br>
        <b>Google Username : </b><br>
        <input type="text" name="google" value=""> <br><br>
        <b>Contact Name : </b><br>
        <!-- <input type="text" name="contacts_id" value=""> <br><br> -->         
        <select class="" name="contacts_id">
      <% name_contact.forEach(contact => { %> 
          <option value="<%= contact.id %>"><%= contact.name %></option>
      <% }) %>
        </select>        
        <br><br>  
              
        <input type="submit" name="" value="SUBMIT">
      </fieldset>
    </form>
  </body>
</html>